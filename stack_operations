class Stack:
    def __init__(self,capacity):
        self.stack = [] 
        self.capacity = capacity
    
    def push(self,elements):
        if self.is_full():
            print("stack is full, we cannot push the elements")
        else:
            self.stack.append(elements)
    
    def pop(self):
        if self.is_empty():
            print("stack is empty")
            return None
        return self.stack.pop()
    
    def peek(self):
        if  self.is_empty():
            print("stack is empty")
            return None
        return self.stack[-1]
    
    def is_empty(self):
        if len(self.stack) == 0:
            return True
            
    def is_full(self):
        if len(self.stack) == self.capacity:
            return True
    def display(self):
        print(self.stack)
            
s = Stack(5)
s.push(1)
s.push(2)
s.push(3)
s.push(4)
s.push(5)
s.pop()
s.display()
print(s.peek())
if s.is_empty():
    s.push(10)
if s.is_full():
    print("stack is full")

            
